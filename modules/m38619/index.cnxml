                              <m:mi>Δ</m:mi>
                            </m:mfrac>
                          </m:mrow>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mfrac>
                            <m:mrow>
                              <m:mtext>24</m:mtext>
                              <m:mo stretchy="false">−</m:mo>
